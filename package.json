{
  "name": "phalanxspace",
  "version": "0.1.0",
  "main": "src/index.js",
  "scripts": {
    "start": "webpack-dev-server",
    "build": "npm run clean && webpack -p",
    "clean": "rm -rf dist",
    "deploy_old": "git add dist && git commit -a -m 'Deployed!' && git push origin :gh-pages && git subtree push --prefix dist origin gh-pages",
    "sync:s3": "AWS_DEFAULT_PROFILE=cezar aws s3 sync dist s3://000pen.space && AWS_DEFAULT_PROFILE=cezar aws s3 sync dist s3://www.000pen.space",
    "sync:cloudfront": "AWS_DEFAULT_PROFILE=cezar aws cloudfront create-invalidation --distribution-id E3IBDZTTC6CYE1 --paths /index.html /bundle.js /bundle.js.map",
    "deploy": "npm run build && npm run sync:s3 && npm run sync:cloudfront"
  },
  "dependencies": {
    "cloudinary-core": "^2.5.0",
    "css-loader": "^1.0.0",
    "navigo": "^7.1.2",
    "p5": "^0.5.9",
    "pify": "^3.0.0",
    "style-loader": "^0.23.1"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "copy-webpack-plugin": "^4.0.1",
    "gh-pages": "^1.1.0",
    "html-webpack-plugin": "^2.28.0",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5"
  }
}
